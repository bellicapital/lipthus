doctype html

html
	head
		style.
			table{border-collapse: collapse;text-align:center}
			table td, th{padding: 4px 8px;border: 1px solid;}
			img{margin: 4px;}
	body
		table
			tr
				th Fecha
				th Query
				th Location
			each s in searches
				tr
					td= s.created.toUserDateString()
					td= s.query
					td= s.location ? s.location.title || s.location.city : 'local'